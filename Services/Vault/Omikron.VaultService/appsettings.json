{
  "Logging": {
    "Use": "Serilog",
    "Seq": {
      "Url": "http://localhost:5341",
      "ApiKey": ""
    },
    "ApplicationInsights": {
      "Key": "",
      "EnableDependencyTrackingTelemetryModule": false,
      "EnableRequestTrackingTelemetryModule": false,
      "EnableSampling": false,
      "CloudRoleName": "VaultService",
      "EnableServiceBusEventsFilter": true
    }
  },
  "VaultPhotoSettings": {
    "MaxSize": "12582912",
    "AllowedFormats": [ ".png", ".jpeg", ".jpg" ]
  },
  "ConnectionStrings": {
    "TenantStoreDb": "",
    "VaultServiceDatabase": ""
  },
  "AzureBlobStorage": {
    "ConnectionString": "UseDevelopmentStorage=true",
    "ContainerName": "vault-service"
  },
  "Azure": {
    "AdAppRegistrationId": "",
    "AdAppRegistrationSecret": "",
    "KeyVaultPath": "",
		"ServiceBus": {
			"ConnectionString": "",
			"Queue": "omikron-vault-queue",
			"MaxConcurrentCalls": 650,
			"PrefetchCount": 1000
		},
		"Redis": {
			"Password": "",
			"Endpoint": "localhost:6379",
			"KeyPrefix": "local"
		},
		"Kestrel": {
			"EndpointDefaults": {
				"Protocols": "Http1AndHttp2"
			}
		}
	},
  "IdentityServer": {
    "Authority": "https://localhost:8082",
    "Audience": "omikron-vault-service-api",
    "ServiceApiSecret": ""
  },
	"Endpoint": {
		"IdentityService": {
			"Uri": "https://localhost:8082/api/v1/"
		}
	},
	"Cors": {
		"Origins": "https://localhost:4200;http://localhost:4200;http://*.local.Omikron.com:4200"
	},
	"ApiServices": {
		"Bud": {
			"ApiUrl": "https://api-sandbox.thisisbud.com",
			"ClientId": "",
			"ClientSecret": "",
			"LoggingStorageConnectionString": "UseDevelopmentStorage=true",
			"Container": "BudApi" 
		}
  },
